{
  "name": "with-chakra-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently --kill-others \"next dev\" \"yarn genwatch\"",
    "build": "next build",
    "start": "next start",
    "gen": "graphql-codegen",
